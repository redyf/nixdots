_: ''
  * {
      font-family: "Liga SFMono Nerd Font", sans-serif;
      font-size: 14px;
      color: white;
      background: transparent;
  }

  #waybar {
      background-color: rgba(28, 28, 30, 0);
      /* background-color: rgba(28, 28, 30, 0.50); */
      border-bottom: 1px solid #000000;
      padding: 4px 10px;
      margin: 5px;
  }

  #clock,
  #network,
  #pulseaudio,
  #cpu,
  #memory,
  #temperature {
      padding: 10px;
      color: white;
  }

  #network,
  #pulseaudio,
  #memory,
  #temperature {
      border-right: 1px solid #444444;
  }

  #clock {
      font-weight: 700;
      font-size: 12px;
  }

  #window {
      padding: 0 5px;
      font-weight: 600;
  }

  #battery {
      color: white;
      background: white;
      border-radius: 15px;
      margin: 5px;
      padding: 2px 10px;
  }

  #custom-launcher {
      color: white;
      padding: 0 20px 0 13px;
      font-size: 23px;
  }

  #custom-text {
      padding-left: 1px;
      font-weight: 600;
  }

  #custom-texttwo,
  #custom-textthree,
  #custom-textfour,
  #custom-textfive {
      font-size: 13px;
      padding-left: 20px;
      color: white;
  }

  #image.ethernetart {
      color: white;
  }

  #custom-date {
      color: white;
      font-size: 13px;
      font-weight: 700;
  }
